		int n = sc.nextInt();
        char temp = 'A';
        temp +=(n-1);
		for(int i=n; i>0; i--){
		   
		   
		   for(int k=1 ; k<=i ; k++){

		       System.out.print(temp);
		   }
		   
		   temp--;
		   
		   System.out.println();
		   
		}
		
		
	}
