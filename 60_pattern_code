////////////////////////////////////////// JAVA CODE PATTERN NUMBER 19 /////////////////////////////////////////

INPUT IS : 5
OUTPUT IS :

E E E E E 
D D D D D
C C C C C
B B B B B
A A A A A 

/// CODE ///


import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		char temp = 'A';
		temp += (n-1);

		for(int  i=n; i>0; i--){
		    
		   for(int k=n; k>0 ; k--){
		       
		       System.out.print(temp+ " ");
		      
		   }
		   temp--;
		   
		   
		    System.out.println();
		   
		}
		
		
	}
}

