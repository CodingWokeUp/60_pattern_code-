import java.util.*;
public class Main
{
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
        
		int n = sc.nextInt();
		
		for(int i=1; i<=n; i++){
		   
		   char temp = 'A';
		   temp += (i-1);
		   for(int k=i; k>=1 ; k--){

		       System.out.print(temp);
		      temp--;
		   }
		    
		   System.out.println();
		   
		}
		
		
	}
}

