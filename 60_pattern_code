import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		
		for(int i=1; i<=n; i++){
		    
		    
		    
		   for(int k=i; k>0 ; k--){
		       
		       System.out.print(k+ " ");
		      
		   }
		   
		    System.out.println();
		   
		}
		
		
	}
}


