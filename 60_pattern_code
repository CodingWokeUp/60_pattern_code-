import java.util.*;
public class Main
{
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		char temp = 'A';

		for(int i=1; i<=n; i++){
		   
		   for(int j=0; j<= n-i; j++){

		       System.out.print(" ");
		       
		   } 
		   
		   for(int k=1 ; k<=i ; k++){

		       System.out.print(temp+" ");
		   }
		   
		   temp++;
		   System.out.println();
		   
		}
		
		
	}
}

