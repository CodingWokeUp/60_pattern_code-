INPUT IS : 5
OUTPUT IS :

EEEEE
 DDDD
  CCC
   BB
    A

/// CODE ///

import java.util.*;
public class Main
{
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		char temp='A';
		temp += (n-1);
		for(int i=1; i<=n; i++){
		    
		   for(int j=1; j<=i; j++){
		       
		       System.out.print(" ");
		   }
		   
		   
		   for(int k=0; k<=n-i ; k++){

		       System.out.print(temp);
		      
		   }
		    temp--;
		   System.out.println();
		   
		}
		
		
	}
}

