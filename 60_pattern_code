public class Main
{
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		
		for(int i=1; i<=n; i++){
		   
		   for(int j=i; j<n; j++){
		       
		       System.out.print(" ");
		   }
		   
		   char temp = 'A';
		   temp += (i-1);
		   for(int k=1; k<=i; k++){

		       System.out.print(temp+" ");
		      temp--;
		   }
		   
		    
