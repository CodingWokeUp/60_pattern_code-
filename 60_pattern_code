import java.util.*;

public class Main
{
	public static void main(String[] args) {
	   
	   Scanner sc = new Scanner(System.in);
	   
	   int n = sc.nextInt();
	   
	   for(int i=0; i<n;i++){
	       
	       for(int j=n-1; j>i; j--){
	           System.out.print("  ");
	       }
	       
	       for(int k=1; k <= i; k++){
	          
	           System.out.print(k+" ");
	       }
	       
	       for(int a=i+1; a>0; a--){
	           
	           System.out.print(a+" ");
	       }
	       
	       System.out.println();
	       
	   }
	    
	}
}

